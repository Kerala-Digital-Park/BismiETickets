<script>
document.addEventListener("DOMContentLoaded", () => {
    document.getElementById("saveNotificationBtn").addEventListener("click", async () => {
        const form = document.getElementById("notificationForm");

        const selectedRadio = form.querySelector('input[name="flexRadioDefault"]:checked')?.id;
        let newsletterValue = "";
switch (selectedRadio) {
    case "flexRadioDefault1": newsletterValue = "Daily"; break;
    case "flexRadioDefault2": newsletterValue = "Twice a week"; break;
    case "flexRadioDefault3": newsletterValue = "Weekly"; break;
    case "flexRadioDefault4": newsletterValue = "Never"; break;
}

        const data = {
            newsletterPreference: newsletterValue, // flexRadioDefault1 / 2 / 3 / 4
            notifyOnLogin: document.getElementById("checkPrivacy1").checked,
            receivePromotions: document.getElementById("checkPrivacy3").checked,
            activateHotelBooking: document.getElementById("checkPrivacy2").checked,
            activateMuqeem: document.getElementById("checkPrivacy7").checked,
            activateDummyItinerary: document.getElementById("checkPrivacy4").checked,
            sendPaymentConfirmation: document.getElementById("checkPrivacy6").checked,
            checkDeviceAccess: document.getElementById("checkPrivacy5").checked,
            userId: document.getElementById("userId").value
        };

        try {
            const response = await fetch("/admin/notification-settings", {
                method: "POST",
                headers: {
                    "Content-Type": "application/json",
                },
                body: JSON.stringify(data),
            });

            const result = await response.json();
            if (result.success) {
                alert("Notification settings saved!");
            } else {
                alert("Failed to save settings.");
            }
        } catch (err) {
            console.error("Error:", err);
            alert("Something went wrong.");
        }
    });
});
</script>

